{
  "name": "airbitz-core-js",
  "version": "0.1.0",
  "description": "Airbitz core Javascript implementation",
  "keywords": [
    "bitcoin",
    "blockchain",
    "ethereum",
    "key management",
    "security"
  ],
  "url": "https://airbitz.co",
  "author": "Airbitz, Inc.",
  "contributors": [
    "Paul Puey <paul@airbitz.co>",
    "William Swanson <william@airbitz.co>",
    "Tim Horton <tim@airbitz.co>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "bin": "bin/airbitz-cli",
  "main": "dist/abc.cjs.js",
  "module": "dist/abc.es6.js",
  "dependencies": {
    "aes-js": "^2.1.0",
    "base-x": "^1.0.4",
    "chalk": "^1.1.3",
    "elliptic": "^6.4.0",
    "hash.js": "^1.0.3",
    "hmac-drbg": "^1.0.0",
    "node-fetch": "^1.6.3",
    "node-getopt": "^0.2.3",
    "node-localstorage": "^1.3.0",
    "scrypt-js": "^2.0.3",
    "source-map-support": "^0.4.6",
    "xdg-basedir": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "chai": "^3.5.0",
    "rollup": "^0.36.1",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "webpack": "^1.14.0"
  },
  "files": [
    "bin/airbitz-cli*",
    "dist/abc.*.js*"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/airbitz-core-js.git"
  },
  "scripts": {
    "build": "webpack && rollup -c && rollup -c rollup.config.cli.js",
    "lint": "eslint '*.js' 'src/**/*.js' 'test/**/*.js'",
    "prepublish": "npm run lint && npm run build",
    "pretest": "npm run lint && webpack && rollup -c rollup.config.test.js",
    "test": "mocha --require source-map-support/register dist/tests.cjs.js",
    "watch": "mocha --watch source-map-support/register"
  }
}
